{
	"name": "@sveltejs/site-kit",
	"version": "6.0.0-next.64",
	"description": "Styles and components for use in *.svelte.dev websites",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"package:watch": "svelte-kit sync && svelte-package --watch",
		"package": "svelte-kit sync && svelte-package"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sveltejs/site-kit.git"
	},
	"author": "Rich Harris",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/sveltejs/site-kit/issues"
	},
	"homepage": "https://github.com/sveltejs/site-kit#readme",
	"dependencies": {
		"esm-env": "^1.0.0",
		"svelte-persisted-store": "^0.9.2"
	},
	"devDependencies": {
		"@sveltejs/kit": "^2.8.3",
		"@sveltejs/package": "^2.3.1",
		"@types/node": "^20.12.11",
		"flexsearch": "^0.7.43",
		"magic-string": "^0.30.10",
		"marked": "^12.0.2",
		"prettier": "^3.2.5",
		"shiki-twoslash": "^3.1.2",
		"svelte": "^4.2.19",
		"typescript": "^5.4.5",
		"vite": "^5.2.11"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1.20.0",
		"svelte": "^4.0.0"
	},
	"types": "./dist/types.d.ts",
	"exports": {
		".": {
			"types": "./dist/types.d.ts"
		},
		"./actions": {
			"types": "./dist/actions/index.d.ts",
			"default": "./dist/actions/index.js"
		},
		"./stores": {
			"types": "./dist/stores/index.d.ts",
			"default": "./dist/stores/index.js"
		},
		"./branding/svelte-logo.svg": "./dist/branding/svelte-logo.svg",
		"./components": {
			"types": "./dist/components/index.d.ts",
			"default": "./dist/components/index.js",
			"svelte": "./dist/components/index.js"
		},
		"./home": {
			"types": "./dist/home/index.d.ts",
			"default": "./dist/home/index.js",
			"svelte": "./dist/home/index.js"
		},
		"./docs": {
			"types": "./dist/docs/index.d.ts",
			"default": "./dist/docs/index.js",
			"svelte": "./dist/docs/index.js"
		},
		"./markdown": {
			"types": "./dist/markdown/index.d.ts",
			"default": "./dist/markdown/index.js",
			"svelte": "./dist/markdown/index.js"
		},
		"./nav": {
			"types": "./dist/nav/index.d.ts",
			"default": "./dist/nav/index.js",
			"svelte": "./dist/nav/index.js"
		},
		"./icons/link.svg": "./dist/icons/link.svg",
		"./icons/search.svg": "./dist/icons/search.svg",
		"./search": {
			"types": "./dist/search/index.d.ts",
			"default": "./dist/search/index.js",
			"svelte": "./dist/search/index.js"
		},
		"./styles/base.css": "./dist/styles/base.css",
		"./styles/code.css": "./dist/styles/code.css",
		"./styles/fonts.css": "./dist/styles/fonts.css",
		"./styles/index.css": "./dist/styles/index.css",
		"./styles/text.css": "./dist/styles/text.css",
		"./styles/tokens.css": "./dist/styles/tokens.css"
	},
	"files": [
		"dist"
	]
}
