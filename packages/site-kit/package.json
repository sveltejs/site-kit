{
	"name": "@sveltejs/site-kit",
	"version": "5.0.0",
	"description": "Styles and components for use in *.svelte.dev websites",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"watch": "svelte-kit sync && svelte-package --watch",
		"package": "svelte-kit sync && svelte-package"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sveltejs/site-kit.git"
	},
	"author": "Rich Harris",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/sveltejs/site-kit/issues"
	},
	"homepage": "https://github.com/sveltejs/site-kit#readme",
	"dependencies": {
		"svelte-local-storage-store": "^0.4.0"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.15.5",
		"@sveltejs/package": "^2.0.2",
		"flexsearch": "^0.7.31",
		"svelte": "^3.58.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^1.0.0",
		"svelte": "^3.54.0"
	},
	"exports": {
		"./actions": {
			"types": "./dist/actions/index.d.ts",
			"default": "./dist/actions/index.js"
		},
		"./branding/svelte-logo.svg": "./dist/branding/svelte-logo.svg",
		"./components/home": {
			"types": "./dist/components/home/index.d.ts",
			"default": "./dist/components/home/index.js"
		},
		"./components": {
			"types": "./dist/components/index.d.ts",
			"default": "./dist/components/index.js"
		},
		"./icons/link.svg": "./dist/icons/link.svg",
		"./icons/search.svg": "./dist/icons/search.svg",
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./search": {
			"types": "./dist/search/index.d.ts",
			"default": "./dist/search/index.js"
		},
		"./search/search-worker": {
			"types": "./dist/search/search-worker.d.ts",
			"default": "./dist/search/search-worker.js"
		},
		"./search/search": {
			"types": "./dist/search/search.d.ts",
			"default": "./dist/search/search.js"
		},
		"./search/stores": {
			"types": "./dist/search/stores.d.ts",
			"default": "./dist/search/stores.js"
		},
		"./search/types.d.ts": "./dist/search/types.d.ts",
		"./styles/base.css": "./dist/styles/base.css",
		"./styles/code.css": "./dist/styles/code.css",
		"./styles/fonts/fira-mono/fira-mono-latin-400.woff2": "./dist/styles/fonts/fira-mono/fira-mono-latin-400.woff2",
		"./styles/fonts/overpass/overpass-latin-100.woff2": "./dist/styles/fonts/overpass/overpass-latin-100.woff2",
		"./styles/fonts/overpass/overpass-latin-300.woff2": "./dist/styles/fonts/overpass/overpass-latin-300.woff2",
		"./styles/fonts/overpass/overpass-latin-400.woff2": "./dist/styles/fonts/overpass/overpass-latin-400.woff2",
		"./styles/fonts/overpass/overpass-latin-600.woff2": "./dist/styles/fonts/overpass/overpass-latin-600.woff2",
		"./styles/fonts/overpass/overpass-latin-700.woff2": "./dist/styles/fonts/overpass/overpass-latin-700.woff2",
		"./styles/fonts/roboto/roboto-latin-400.woff2": "./dist/styles/fonts/roboto/roboto-latin-400.woff2",
		"./styles/fonts/roboto/roboto-latin-400italic.woff2": "./dist/styles/fonts/roboto/roboto-latin-400italic.woff2",
		"./styles/fonts/roboto/roboto-latin-500.woff2": "./dist/styles/fonts/roboto/roboto-latin-500.woff2",
		"./styles/fonts/roboto/roboto-latin-500italic.woff2": "./dist/styles/fonts/roboto/roboto-latin-500italic.woff2",
		"./styles/fonts.css": "./dist/styles/fonts.css",
		"./styles/index.css": "./dist/styles/index.css",
		"./styles/text.css": "./dist/styles/text.css",
		"./styles/tokens.css": "./dist/styles/tokens.css",
		"./theme": {
			"types": "./dist/theme/index.d.ts",
			"default": "./dist/theme/index.js"
		}
	},
	"files": [
		"dist"
	]
}
